{
  "name": "netlify-cms-site",
  "version": "1.0.0",
  "description": "Netlify CMS documentation website built with Gatsby",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build && rm -rf dist && mv public dist",
    "lint": "markdownlint 'content/docs/**/*.md'",
    "copy:contribs": "cp ../.all-contributorsrc data/contributors.json",
    "prepare": "npm run copy:contribs",
    "reset": "rm -rf .cache"
  },
  "author": "",
  "homepage": "https://www.netlifycms.org/",
  "license": "MIT",
  "dependencies": {
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^10.3.1",
    "@emotion/styled": "^10.3.0",
    "dayjs": "^1.10.7",
    "emotion-theming": "^10.3.0",
    "gatsby": "3.14.6",
    "gatsby-plugin-catch-links": "3.14.0",
    "gatsby-plugin-emotion": "^4.5.0",
    "gatsby-plugin-manifest": "3.14.0",
    "gatsby-plugin-netlify-cms": "5.14.0",
    "gatsby-plugin-react-helmet": "4.14.0",
    "gatsby-remark-autolink-headers": "4.11.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-prismjs": "5.11.0",
    "gatsby-source-filesystem": "3.14.0",
    "gatsby-transformer-json": "3.14.0",
    "gatsby-transformer-remark": "4.11.0",
    "gatsby-transformer-yaml": "3.14.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "netlify-cms-app": "^2.15.63",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-github-btn": "^1.2.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^6.0.3",
    "smooth-scroll": "^16.1.3"
  },
  "devDependencies": {
    "babel-plugin-prismjs": "^2.1.0",
    "babel-preset-gatsby": "^1.14.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.4",
    "markdownlint-cli": "^0.30.0"
  },
  "private": true
}
