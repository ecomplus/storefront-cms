    "immutable": "^3.8.2",
    "netlify-cms-ui-default": "^2.12.1",
    "prop-types": "^15.8.0",
    "react": "^16.8.4 || ^17.0.0",
    "react-immutable-proptypes": "^2.2.0",
    "uuid": "^3.4.0"
  }
