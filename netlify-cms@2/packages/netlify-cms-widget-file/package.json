  "module": "dist/esm/index.js",
  "main": "dist/netlify-cms-widget-file.js",
  "license": "MIT",
  "keywords": [
    "netlify",
    "netlify-cms",
    "widget",
    "file",
    "upload",
    "file-upload"
  ],
